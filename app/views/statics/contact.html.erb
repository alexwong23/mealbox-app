
     <%= form_for(:session, url: contact_path) do |f| %>

     <%= f.label :Name %>
     <%= f.text_field :email, class: 'form-control' %>

     <%= f.label :email %>
     <%= f.email_field :email, class: 'form-control' %>

     <%= f.label :Message %>
     <%= f.textarea_field :password, class: 'form-control' %>

     <%= f.submit "Submit", class: "btn btn-primary" %>
   <% end %>
